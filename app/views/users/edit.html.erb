<h1>設定変更</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@user) do |f| %>

      <%= f.label :contact_email %>
      <%= f.text_field :contact_email %>

      <%= f.label :jp_market_place_id %>
      <%= f.text_field :jp_market_place_id %>

      <%= f.label :jp_markchant_id %>
      <%= f.text_field :jp_markchant_id %>
      
      <%= f.label :jp_access_key_id %>
      <%= f.text_field :jp_access_key_id %>

      <%= f.label :jp_secret_key %>
      <%= f.text_field :jp_secret_key %>

      <%= f.label :com_market_place_id %>
      <%= f.text_field :com_market_place_id %>

      <%= f.label :com_markchant_id %>
      <%= f.text_field :com_markchant_id %>
      
      <%= f.label :com_access_key_id %>
      <%= f.text_field :com_access_key_id %>

      <%= f.label :com_secret_key %>
      <%= f.text_field :com_secret_key %>

      <%= f.submit "更新", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>